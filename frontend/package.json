{
  "name": "linlin-frontend",
  "version": "1.0.0",
  "description": "LinLin Frontend - A Dioxus and Tailwind CSS project",
  "scripts": {
    "tailwind": "tailwindcss -i ./input.css -o ./assets/tailwind.css --watch",
